{
  "version": "1",
  "flow_name": "demo-flow",
  "update_date": "2024-04-26",
  "create_date": "2024-04-01",
  "developer": "Tine aine",
  "requirements": [
    {
      "name": "engine",
      "env_type": "Base",
      "ver": "1.0.0"
    },
    {
      "name": "java",
      "env_type": "RT",
      "ver": "1.8.0"
    },
    {
      "name": "core",
      "env_type": "Ext",
      "ver": "0.1.0"
    }
  ],
  "steps": [
    {
      "handler": "simx.files.plain.reader",
      "attr": {
        "node_id": "150245215350",
        "node_name": "读取文件",
        "node_type": "raw",
        "ignore": "none",
        "path": "conf/simx.toml"
      }
    },
    {
      "handler": "simx.os.shell.println",
      "attr": {
        "node_id": "150245215351",
        "node_name": "打印信息",
        "node_type": "raw",
        "ignore": "none",
        "text": "hello world"
      }
    },
    {
      "handler": "simx.script",
      "attr": {
        "node_id": "150245215352",
        "node_name": "执行脚本",
        "node_type": "raw",
        "ignore": "none",
        "path": "script/os/macos/b.sh"
      }
    },
    {
      "handler": "simx.basic.flow.sub_flow",
      "attr": {
        "node_id": "150245215359",
        "node_name": "子流程",
        "node_type": "raw",
        "ignore": "none",
        "steps": "[{\"handler\": \"simx.os.shell.println\",\"attr\":{\"node_id\": \"150245215351\",\"node_name\": \"打印信息\",\"node_type\": \"raw\",\"ignore\": \"none\",\"text\": \"hello world in subflow\"}},{\"handler\": \"simx.script\",\"attr\":{\"node_id\": \"150245215352\",\"node_name\": \"执行脚本\",\"node_type\": \"raw\",\"ignore\": \"none\",\"path\": \"script/os/windows/test.ps1\"}}]"
      }
    },
    {
      "handler": "simx.basic.flow.post_flow",
      "attr": {
        "node_id": "150245215351",
        "node_name": "发起流程",
        "node_type": "raw",
        "ignore": "none",
        "flow_path": "flow/test.flow"
      }
    },
    {
      "handler": "simx.files.plain.writer",
      "attr": {
        "node_id": "150245215353",
        "node_name": "写文件",
        "node_type": "raw",
        "ignore": "none",
        "path": "tmp/sss.toml",
        "type": "default",
        "content": "text",
        "override": "force"
      }
    },
    {
      "handler": "db.test",
      "attr": {
        "node_id": "150245215354",
        "node_name": "测试方法",
        "node_type": "ext",
        "ignore": "err",
        "a": "30",
        "b": "20"
      }
    }
  ]
}