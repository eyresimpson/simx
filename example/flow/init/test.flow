{
  "version": "1",
  "name": "测试流",
  "update_date": "2024-10-18",
  "create_date": "2024-09-27",
  "developer": "Noah Jones",
  "requirements": [
    {
      "name": "engine",
      "env_type": "Base",
      "version": "1.0.0"
    },
    {
      "name": "java",
      "env_type": "Runtime",
      "version": "1.8.0"
    }
  ],
  "blueprint": {
    "parallel_endpoints": false,
    "parallel_routes": false,
    "maximum_repetition": 30,
    "endpoints": [
      "1"
    ],
    "routes": {
      "1": {
        "name": "打印信息",
        "handler": "simx.core.debug.print",
        "downstream": [
          {
            "expr": "1==1",
            "target": "2"
          }
        ],
        "redress_stream": [],
        "attr": {
          "text": 123,
          "level": "info"
        }
      },
      "2": {
        "name": "设置变量",
        "handler": "simx.core.var.set",
        "downstream": [
          "5"
        ],
        "redress_stream": [],
        "attr": {
          "key": "aaa",
          "val": "1"
        }
      },
      "5": {
        "name": "打印信息1",
        "handler": "simx.core.debug.print",
        "downstream": [
          {
            "expr": "1==${aaa}",
            "target": "6"
          }
        ],
        "redress_stream": [],
        "attr": {
          "text": "hello world in println 5"
        }
      },
      "6": {
        "name": "创建文件夹",
        "handler": "simx.files.dir.create",
        "downstream": [
          "7"
        ],
        "redress_stream": [],
        "attr": {
          "path": "./helloworld"
        }
      },
      "7": {
        "name": "复制文件夹",
        "handler": "simx.files.dir.cp",
        "downstream": [
          "8"
        ],
        "redress_stream": [],
        "attr": {
          "source": "./script",
          "target": "./test"
        }
      },
      "8": {
        "name": "流等待",
        "handler": "simx.core.flow.timeout",
        "downstream": [],
        "redress_stream": [],
        "attr": {
          "sec": 5.5
        }
      }
    }
  }
}