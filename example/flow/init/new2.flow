{
  "version": "2",
  "name": "demo-flow",
  "update_date": "2024-09-27",
  "create_date": "2024-04-01",
  "developer": "Tine aine",
  "requirements": [
    {
      "name": "engine",
      "env_type": "Base",
      "ver": "1.0.0"
    },
    {
      "name": "java",
      "env_type": "RT",
      "ver": "1.8.0"
    },
    {
      "name": "core",
      "env_type": "Ext",
      "ver": "0.1.0"
    }
  ],
  "blueprint": {
    "parallel_endpoints": true,
    "parallel_routes": false,
    "maximum_repetition": 60,
    "endpoints": [
      "2"
    ],
    "routes": {
      "1": {
        "name": "读取文件",
        "handler": "simx.files.plain.reader",
        "downstream": [
          "2"
        ],
        "redress_stream": [],
        "attr": {
          "path": "conf/simx.toml"
        }
      },
      "2": {
        "name": "打印信息",
        "handler": "simx.os.shell.println",
        "downstream": [
          "3"
        ],
        "redress_stream": [],
        "attr": {
          "text": "hello world in println"
        }
      },
      "3": {
        "name": "执行脚本",
        "handler": "simx.script",
        "downstream": [],
        "redress_stream": [],
        "attr": {
          "path": "script/os/macos/b.sh"
        }
      },
      "4": {
        "name": "goto",
        "handler": "simx.core.route.goto",
        "downstream": [],
        "redress_stream": [],
        "tags": [
          "Route"
        ],
        "attr": {
          "overrun_fault": "超出goto跳跃限制，流已强制停止",
          "router": "[{\"target\": \"2\", \"expression\": \"true\"}]"
        }
      }
    }
  }
}