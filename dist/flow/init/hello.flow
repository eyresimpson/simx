{
  "version": "1",
  "flow_name": "first-flow",
  "update_date": "2024-04-01",
  "create_date": "2024-04-01",
  "developer": "Tine aine",
  "env_req": [
    {
      "env_name": "engine",
      "env_type": "1.0.0"
    },
    {
      "env_name": "java",
      "env_type": "1.8.0"
    }
  ],
  "nodes": [
    {
      "mold": "ORIGIN",
      "handler": "simx.origin.file.plain",
      "attr": {},
      "steps": [
        {
          "func": "file_reader",
          "attr": {
            "path": "conf/simx.toml"
          }
        }
      ]
    },
    {
      "mold": "EXEC",
      "handler": "simx.exec.system.shell",
      "attr": {
        "hello": "Morning!"
      },
      "steps": [
        {
          "func": "println",
          "attr": {
            "text": "hello world"
          }
        }
      ]
    },
    {
      "mold": "ENDPOINT",
      "handler": "simx.endpoint.file.json",
      "attr": {
        "test": "Func"
      },
      "steps": []
    }
  ]
}